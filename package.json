{
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/drazik/blog.git"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "git-directory-deploy": "1.5.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "npm run build",
    "deploy": "git-directory-deploy --directory dist --branch build --repo=git@github.com:drazik/blog.git",
    "now-build": "npm run build",
    "lint": "eslint --fix src/**/*.{js,jsx}"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@svgr/webpack": "^5.2.0",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.19.32",
    "gatsby-plugin-emotion": "^4.1.24",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-remark": "^2.7.0",
    "lodash": "^4.17.15",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "5.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  }
}
